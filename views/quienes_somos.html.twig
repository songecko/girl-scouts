{% extends "layout.html.twig" %}

{% block content %}
<section>
	<div id="carousel" class="carousel slide">
		<ol class="carousel-indicators text-center">
			<li data-target="#carousel" data-slide-to="0" class="active"></li>
			<li data-target="#carousel" data-slide-to="1"></li>
			<!--<li data-target="#carousel" data-slide-to="2"></li>-->
		</ol>
		<div class="carousel-inner">
        	<div class="active item"><img src="{{ app.request.basepath }}/img/quienes-somos-03.jpg" class="fullwidth" /></div>
			<div class="item"><img src="{{ app.request.basepath }}/img/quienes-somos-02.jpg" class="fullwidth" /></div>
			<!-- <div class="item"><img src="img/home-03.jpg" /></div>-->
		</div>
                      
		<a class="carousel-control left" href="#carousel" data-slide="prev"><img src="{{ app.request.basepath }}/img/left.png" width="52" height="72"></a>
		<a class="carousel-control right" href="#carousel" data-slide="next"><img src="{{ app.request.basepath }}/img/right.png" width="52" height="72"></a>
	</div>
</section>
{% endblock %}